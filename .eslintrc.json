{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:wordpress/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "quotes": ["error", "double"],
      "semi": ["error", "always"]
    },
    "scripts": {
        "lint": "eslint . --fix"
      }
  }
  